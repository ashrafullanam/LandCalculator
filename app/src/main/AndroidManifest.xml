<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ashraf.landcalculator">

    <application
        android:allowBackup="true"
        android:icon="@drawable/measurement"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".QuardView"
            android:screenOrientation="portrait"/>
        <activity android:name=".QuardViewShow"
            android:screenOrientation="portrait"/>
        <activity android:name=".TriangleView"
            android:screenOrientation="portrait"/>
        <activity android:name=".TriangleViewShow"
            android:screenOrientation="portrait"></activity>
    </application>

</manifest>